<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:COM_DEBUGGER"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d" x:Class="COM_DEBUGGER.MainWindow"
        xmlns:local2="clr-namespace:WPFAutoCompleteTextbox"
        Title="ComDebugger V3.8" Margin="1,0,0,0" MinWidth="650" MinHeight="650" Width="1100" Height="750" WindowStartupLocation="CenterScreen" Icon="logo.ico" Visibility="Visible">
    <Window.Resources>
        <ControlTemplate x:Key="ScrollViewerControlTemplate1" TargetType="{x:Type ScrollViewer}">
            <Grid x:Name="Grid" Background="{TemplateBinding Background}">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="Auto"/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="Auto"/>
                </Grid.RowDefinitions>
                <Rectangle x:Name="Corner" Grid.Column="1" Fill="#FF7C7C7C" Grid.Row="1"/>
                <ScrollContentPresenter x:Name="PART_ScrollContentPresenter" CanContentScroll="{TemplateBinding CanContentScroll}" CanHorizontallyScroll="False" CanVerticallyScroll="False" ContentTemplate="{TemplateBinding ContentTemplate}" Content="{TemplateBinding Content}" Grid.Column="0" Margin="{TemplateBinding Padding}" Grid.Row="0" d:IsHidden="True"/>
                <ScrollBar x:Name="PART_VerticalScrollBar" AutomationProperties.AutomationId="VerticalScrollBar" Cursor="Arrow" Grid.Column="1" Maximum="{TemplateBinding ScrollableHeight}" Minimum="0" Grid.Row="0" Visibility="{TemplateBinding ComputedVerticalScrollBarVisibility}" Value="{Binding VerticalOffset, Mode=OneWay, RelativeSource={RelativeSource TemplatedParent}}" ViewportSize="{TemplateBinding ViewportHeight}" Background="#FF686868" Width="Auto" MinWidth="14"/>
            </Grid>
        </ControlTemplate>
    </Window.Resources>
    <Grid x:Name="BigGrid" Background="#FF3E3E42">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="235*"/>
            <ColumnDefinition Width="235*"/>
            <ColumnDefinition Width="130*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="8*"/>
            <RowDefinition Height="5*"/>
            <RowDefinition Height="167*"/>
        </Grid.RowDefinitions>
        <ToolBarPanel x:Name="MenuBar" Background="#FF3E3E42" VerticalAlignment="Top" Height="24" Grid.ColumnSpan="3">
            <Menu HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Background="{x:Null}" Foreground="White" VerticalAlignment="Center" Padding="0" FontSize="14" Margin="10,0,0,0" Width="396" HorizontalAlignment="Left">
                <MenuItem Header="File(F)" Background="{x:Null}" Foreground="White" Padding="0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" FontSize="16" Width="50">
                    <MenuItem Header="Exit" Background="#FF2D2D30" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}" Click="Exit_Click"/>
                </MenuItem>
                <MenuItem Header="Settings(S)" Foreground="White" Padding="3,0" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="16">
                    <MenuItem Background="#FF2D2D30" Header="Options" Click="Options_Click"/>
                    <MenuItem Background="#FF2D2D30" Header="Open Command Files Location" Click="UpdateCommands_Click"/>
                </MenuItem>
                <MenuItem Header="Help(H)" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="16">
                    <MenuItem Header="View Help"  Background="#FF2D2D30" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}" Click="Help_Click"/>
                    <MenuItem Header="Software Evaluation" Background="#FF2D2D30" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}" Click="Evaluation_Click"/>
                </MenuItem>
                <MenuItem Header="About(A)" Click="About_Click" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="16"/>
            </Menu>
        </ToolBarPanel>
        <ToolBarPanel x:Name="ToolBar2" Margin="0" Grid.Row="1" Grid.Column="1">
            <Menu x:Name="menu2" Background="#FF3E3E42" Foreground="#FFE6DFDF" HorizontalAlignment="Left" BorderBrush="#FFD8D8D8" VerticalAlignment="Center" Margin="0">
                <MenuItem x:Name="ClearRMenu" Header="Clear Right" Click="Clear_Serial1_Click"/>
                <MenuItem x:Name="ClearAllMenu2" Header="Clear All" Click="Clear_All_Click"/>
                <MenuItem Header="Save"/>
                <CheckBox x:Name="CPLD_Check" Content="Start '$'" HorizontalAlignment="Left" Foreground="White" Background="White" Click="CPLD_Check_Click"/>
                <CheckBox x:Name="NewLineCheck2" Content="End '\r\n'" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White" Click="SendNewLineChecked" Background="White" BorderBrush="#FF3F3F46" Padding="0"/>
            </Menu>
        </ToolBarPanel>
        <ToolBarPanel x:Name="ToolBar1" Margin="0" Grid.Row="1">
            <Menu x:Name="menu1" Background="#FF3E3E42" Foreground="#FFE6DFDF" BorderBrush="#FFD8D8D8" VerticalAlignment="Center" Margin="0" HorizontalAlignment="Left">
                <MenuItem x:Name="ClearLMenu" Header="Clear Left" Click="Clear_Serial1_Click"/>
                <MenuItem x:Name="ClearAllMenu1" Header="Clear All" Click="Clear_All_Click"/>
                <MenuItem Header="Save"/>
                <CheckBox x:Name="NewLineCheck1" Content="End '\r\n'" HorizontalAlignment="Center" Margin="0" VerticalAlignment="Center" Foreground="White" Click="SendNewLineChecked" Background="White" BorderBrush="#FF3F3F46" Padding="0"/>
            </Menu>
        </ToolBarPanel>
        <Grid x:Name="GroupGrid1" Margin="0,0,2,0" Grid.Row="2" SnapsToDevicePixels="True" Background="#FF3E3E42">
            <Grid.RowDefinitions>
                <RowDefinition Height="520*"/>
                <RowDefinition Height="36*"/>
                <RowDefinition Height="119*"/>
            </Grid.RowDefinitions>
            <Rectangle Margin="0" Stroke="#FF686868"/>
            <ScrollViewer x:Name="serial1" Margin="0" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" VerticalScrollBarVisibility="Auto" BorderThickness="1" TabIndex="-1" Template="{DynamicResource ScrollViewerControlTemplate1}">
                <RichTextBox x:Name="textbox1" Background="#FF252526" BorderBrush="#FFA0A0A0" Foreground="White" IsReadOnly="True" Padding="10" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" FontSize="13.333" >
                    <FlowDocument LineHeight="1" PagePadding="0" IsColumnWidthFlexible="False">
                        <Paragraph>
                            <Run Text=""/>
                        </Paragraph>
                    </FlowDocument>
                </RichTextBox>
            </ScrollViewer>
            <local2:AutoCompleteTextBox x:Name="sendbox1" Grid.Row="1" KeyDown="sendbox1_KeyDown" FrontSize="16"  FontWeight="Bold" FontFamily="Microsoft JhengHei UI"  PreviewKeyDown="sendbox2_PreviewKeyDown"/>
            <!--<TextBox x:Name="sendbox1" Margin="0" TextWrapping="Wrap" UndoLimit="20" KeyDown="sendbox1_KeyDown"  VerticalContentAlignment="Center" HorizontalScrollBarVisibility="Auto" Grid.Row="1" BorderBrush="{x:Null}" Background="#FFDDDDDD" FontWeight="Bold" FontFamily="Microsoft JhengHei UI" Padding="0" SelectionBrush="#FF5F90C2"/>-->
            <Grid x:Name="ContolPanel1" Margin="0" Grid.Row="2" Background="#FF007ACC">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="292*"/>
                    <ColumnDefinition Width="87*"/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="22*"/>
                    <RowDefinition Height="17*"/>
                </Grid.RowDefinitions>
                <WrapPanel x:Name="PORTS_BAUDRATE1" VerticalAlignment="Center" Margin="10,18,0,18" Height="30">
                    <WrapPanel x:Name="WrapPorts1" HorizontalAlignment="Center" VerticalAlignment="Center" Height="30" Width="150">
                        <Label Content="Port Name" HorizontalAlignment="Center" VerticalAlignment="Center" RenderTransformOrigin="0.5,0.5" Background="{x:Null}" Foreground="White" FontWeight="Bold" HorizontalContentAlignment="Right" VerticalContentAlignment="Center" Padding="0,5,2,5" Margin="0" Height="30" Width="63"/>
                        <ComboBox x:Name="comboBox_Ports1" Height="25" VerticalAlignment="Center" SelectionChanged="Ports_SelectionChanged" Width="72" Background="White" HorizontalAlignment="Center" VerticalContentAlignment="Center" Padding="6,0,0,0"/>
                    </WrapPanel>
                    <WrapPanel x:Name="WrapBaudrate1" VerticalAlignment="Center" HorizontalAlignment="Left" Height="30" Width="150">
                        <Label Content="Baud Rate" HorizontalAlignment="Center" VerticalAlignment="Center" Height="30" Foreground="#FFFBFBFB" FontWeight="Bold" HorizontalContentAlignment="Right" VerticalContentAlignment="Center" Padding="0,5,2,5" ScrollViewer.CanContentScroll="True" Width="63"/>
                        <ComboBox x:Name="comboBox_BaudRate1" HorizontalAlignment="Center" VerticalAlignment="Center" Width="72" Height="25" SelectionChanged="comboBox_BaudRate_SelectionChanged" Background="White" Padding="6,0,0,0" VerticalContentAlignment="Center"/>
                    </WrapPanel>
                </WrapPanel>
                <WrapPanel x:Name="OTHERS_CONFIG1" Margin="8,0,0,0" Grid.Row="1" VerticalAlignment="Center" Height="38">
                    <WrapPanel x:Name="StopBits_panel1" VerticalAlignment="Center" Width="73" HorizontalAlignment="Center">
                        <Label x:Name="StopBist1" Content="Stop Bits" HorizontalAlignment="Center" VerticalContentAlignment="Center" Padding="7,0,0,0" ScrollViewer.VerticalScrollBarVisibility="Disabled" VerticalAlignment="Center" Foreground="White"/>
                        <ComboBox x:Name="comboBox_StopBits1" Width="67" HorizontalAlignment="Right" SelectionChanged="comboBox_StopBits_SelectionChanged" VerticalContentAlignment="Center" Padding="5,3,0,3" Background="White" VerticalAlignment="Center" Height="23"/>
                    </WrapPanel>
                    <WrapPanel x:Name="DataBits_panel1" VerticalAlignment="Center" Width="73">
                        <Label x:Name="DataBits1" Content="Data Bits" HorizontalAlignment="Center" Padding="7,0,0,0" VerticalContentAlignment="Center" VerticalAlignment="Center" Foreground="White"/>
                        <ComboBox x:Name="comboBox_Databits1" Padding="5,3,0,3" SelectionChanged="comboBox_Databits_SelectionChanged" VerticalContentAlignment="Center" Background="White" Width="67" Height="23"/>
                    </WrapPanel>
                    <WrapPanel x:Name="Parity_panel1" VerticalAlignment="Center" Width="73">
                        <Label x:Name="label1" Content="Parity" HorizontalAlignment="Center" VerticalAlignment="Center" Padding="7,0,0,0" VerticalContentAlignment="Center" Foreground="White"/>
                        <ComboBox x:Name="comboBox_Parity1" HorizontalAlignment="Left" Width="67" Padding="5,0,0,0" VerticalAlignment="Center" SelectionChanged="comboBox_Parity_SelectionChanged" VerticalContentAlignment="Center" Background="White" Height="23"/>
                    </WrapPanel>
                    <WrapPanel x:Name="HandShake_panel1" VerticalAlignment="Center" Width="73">
                        <Label x:Name="handshake1" Content="Flow Control" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Right" VerticalContentAlignment="Center" Padding="0,0,4,0" Foreground="#FFFFF6F6"/>
                        <ComboBox x:Name="comboBox_HandShake1" HorizontalAlignment="Left" Width="67" Padding="5,0,0,0" Height="23" VerticalAlignment="Center" SelectionChanged="comboBox_HandShake_Selected" VerticalContentAlignment="Center" Background="White"/>
                    </WrapPanel>
                </WrapPanel>
                <Button x:Name="Op_Button1" Content="Open" Click="Button_Open1" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0" Background="White" FontWeight="Bold" Grid.Column="1" Padding="15,13" Grid.RowSpan="2" />
            </Grid>
        </Grid>
        <Grid x:Name="Group3" Grid.Column="2" Margin="3,0,0,0" Grid.RowSpan="3">
            <Grid.RowDefinitions>
                <RowDefinition Height="28*"/>
                <RowDefinition Height="410*"/>
                <RowDefinition Height="2*"/>
                <RowDefinition Height="150*"/>
            </Grid.RowDefinitions>
            <Rectangle Stroke="#FF0C87D8" Margin="0,0.8,-0.4,-0.2" Grid.Row="3"/>
            <Grid x:Name="portwindow" Margin="0,0.8,-0.4,-0.2" Grid.Row="3" Background="#FF007ACC">
                <Grid.RowDefinitions>
                    <RowDefinition Height="138*"/>
                    <RowDefinition Height="25*"/>
                </Grid.RowDefinitions>
                <Grid Margin="0" Grid.Row="1">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="82*"/>
                        <ColumnDefinition Width="61*"/>
                    </Grid.ColumnDefinitions>
                    <Button x:Name="RefreshButton" Content="refresh" ClickMode="Press" BorderBrush="{x:Null}" Margin="48,0,10,0" RenderTransformOrigin="1.132,22.288" HorizontalAlignment="Center" Background="{x:Null}"  Click="RefreshButton_Click_1" Padding="2" Grid.Column="1" FontSize="11" Foreground="#FFEAE5E5"/>
                    <Label Content="Ports List" HorizontalAlignment="Left" Width="65" Foreground="White" Margin="0" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="11" Padding="2" VerticalAlignment="Center"/>
                </Grid>
                <ListBox x:Name="PortListBox" Margin="0" ScrollViewer.VerticalScrollBarVisibility="Auto" BorderBrush="{x:Null}"/>
            </Grid>
            <TabControl x:Name="CommandSource" AllowDrop="True" Margin="0,0.6,-0.4,0.6" BorderBrush="#FF3896D4" Background="White" Grid.Row="1" Foreground="{x:Null}">
                <TabItem Margin="0" BorderBrush="#FF007ACC" Header="Scripts" Background="#FF3896D4" RenderTransformOrigin="0.5,0.5" FontSize="14">
                    <TabItem.RenderTransform>
                        <TransformGroup>
                            <ScaleTransform ScaleY="1" ScaleX="1"/>
                            <SkewTransform AngleY="0" AngleX="0"/>
                            <RotateTransform Angle="0"/>
                            <TranslateTransform/>
                        </TransformGroup>
                    </TabItem.RenderTransform>
                    <Grid Margin="0">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="317*"/>
                            <RowDefinition Height="39*"/>
                        </Grid.RowDefinitions>
                        <Grid Margin="0" Grid.Row="1">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="50*"/>
                                <ColumnDefinition Width="50*"/>
                                <ColumnDefinition Width="50*"/>
                                <ColumnDefinition Width="50*"/>
                            </Grid.ColumnDefinitions>
                            <Button x:Name="ADDButton" Content="Files" Margin="0" Click="ADDButton_Click" Background="{x:Null}" BorderBrush="#FF0C87D8" Padding="0" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="12" Width="40" Height="25"/>
                            <Button x:Name="AddFolder" Content="Folder" Margin="0" Click="AddFolder_Click" Background="{x:Null}" BorderBrush="#FF0C87D8" Padding="0" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="12" Width="40" Height="25" Grid.Column="1"/>
                            <Button x:Name="ViewButton" Content="View" Margin="0" Click="ViewButton_Click" Background="{x:Null}" BorderBrush="#FF0C87D8" Padding="0" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="12" Width="40" Height="25" Grid.Column="2" IsEnabled="False"/>
                            <Button x:Name="RUNButton" Content="Run" Margin="0" Click="RUNButton_Click" Background="{x:Null}" BorderBrush="#FFD66355" Padding="0" Grid.Column="3" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="12" BorderThickness="2" Width="40" Height="25"/>
                        </Grid>
                        <ListBox x:Name="ScriptsList" IsSynchronizedWithCurrentItem="True" Background="White" Foreground="Black" BorderBrush="{x:Null}" Margin="0" MouseDoubleClick="ScriptsList_MouseDoubleClick" AllowDrop="True" Drop="ListBox_Drop" DisplayMemberPath="Name" SelectedValuePath="FilePath" SnapsToDevicePixels="True" SelectionChanged="ScriptsList_SelectionChanged"/>
                    </Grid>
                </TabItem>
                <TabItem Header=" CPLD" BorderBrush="#FF3896D4" Margin="0" Background="#FF3896D4" FontSize="14.667">
                    <Grid x:Name="CPLD" >
                        <ListBox x:Name="CPLDList" Margin="0" MouseDoubleClick="CommandList_MouseDoubleClick" BorderBrush="{x:Null}"/>
                    </Grid>
                </TabItem>
                <TabItem Header="KABS" Margin="0" Background="#FF3896D4" BorderBrush="#FF3896D4" FontSize="14.667">
                    <Grid>
                        <TreeView x:Name="treeView" Margin="0" MouseDoubleClick="treeView_MouseDoubleClick" FontSize="13.333" ToolTip="KABS command">
                            <TreeView.ItemTemplate>
                                <HierarchicalDataTemplate DataType ="{x:Type local:CMDNode}" ItemsSource="{Binding NextNode}">
                                    <TextBlock Text="{Binding NodeName}" ToolTip="{Binding NodeName}" Tag="{Binding NodeName}"/>
                                </HierarchicalDataTemplate>
                            </TreeView.ItemTemplate>
                        </TreeView>

                    </Grid>

                </TabItem>
            </TabControl>
        </Grid>
        <Grid x:Name="GroupGrid2" Margin="0,0,2,0" Grid.Row="2" SnapsToDevicePixels="True" Background="#FF3E3E42" Grid.Column="1">
            <Grid.RowDefinitions>
                <RowDefinition Height="520*"/>
                <RowDefinition Height="36*"/>
                <RowDefinition Height="119*"/>
            </Grid.RowDefinitions>
            <Rectangle Margin="0" Stroke="#FF686868"/>
            <ScrollViewer x:Name="serial2" Margin="0" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" VerticalScrollBarVisibility="Auto" BorderThickness="1" TabIndex="-1" Template="{DynamicResource ScrollViewerControlTemplate1}">
                <RichTextBox x:Name="textbox2" Background="#FF252526" BorderBrush="#FFA0A0A0" Foreground="White" IsReadOnly="True" Padding="10" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" >
                    <FlowDocument LineHeight="1" PagePadding="0" IsColumnWidthFlexible="False">
                        <Paragraph>
                            <Run Text=""/>
                        </Paragraph>
                    </FlowDocument>
                </RichTextBox>
            </ScrollViewer>
            <local2:AutoCompleteTextBox x:Name="sendbox2" Grid.Row="1" KeyDown="sendbox1_KeyDown" FrontSize="16" FontFamily="Microsoft JhengHei UI" PreviewKeyDown="sendbox2_PreviewKeyDown"/>
            <!--<TextBox x:Name="sendbox2" Margin="0" TextWrapping="Wrap" UndoLimit="20" KeyDown="sendbox1_KeyDown" FontSize="16" VerticalContentAlignment="Center" HorizontalScrollBarVisibility="Auto" Grid.Row="1" BorderBrush="{x:Null}" Background="#FFDDDDDD" FontWeight="Bold" FontFamily="Microsoft JhengHei UI" Padding="0" SelectionBrush="#FF5F90C2"/>-->
            <Grid x:Name="ContolPanel2" Margin="0" Grid.Row="2" Background="#FF007ACC">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="292*"/>
                    <ColumnDefinition Width="87*"/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="22*"/>
                    <RowDefinition Height="17*"/>
                </Grid.RowDefinitions>
                <WrapPanel x:Name="PORTS_BAUDRATE2" VerticalAlignment="Center" Margin="10,18,0,18" Height="30">
                    <WrapPanel x:Name="WrapPorts2" HorizontalAlignment="Center" VerticalAlignment="Center" Height="30" Width="150">
                        <Label Content="Port Name" HorizontalAlignment="Center" VerticalAlignment="Center" RenderTransformOrigin="0.5,0.5" Background="{x:Null}" Foreground="White" FontWeight="Bold" HorizontalContentAlignment="Right" VerticalContentAlignment="Center" Padding="0,5,2,5" Margin="0" Height="30" Width="63"/>
                        <ComboBox x:Name="comboBox_Ports2" Height="25" VerticalAlignment="Center" SelectionChanged="Ports_SelectionChanged" Width="72" Background="White" HorizontalAlignment="Center" VerticalContentAlignment="Center" Padding="6,0,0,0"/>
                    </WrapPanel>
                    <WrapPanel x:Name="WrapBaudrate2" VerticalAlignment="Center" HorizontalAlignment="Left" Height="30" Width="150">
                        <Label Content="Baud Rate" HorizontalAlignment="Center" VerticalAlignment="Center" Height="30" Foreground="#FFFBFBFB" FontWeight="Bold" HorizontalContentAlignment="Right" VerticalContentAlignment="Center" Padding="0,5,2,5" ScrollViewer.CanContentScroll="True" Width="63"/>
                        <ComboBox x:Name="comboBox_BaudRate2" HorizontalAlignment="Center" VerticalAlignment="Center" Width="72" Height="25" SelectionChanged="comboBox_BaudRate_SelectionChanged" Background="White" Padding="6,0,0,0" VerticalContentAlignment="Center"/>
                    </WrapPanel>
                </WrapPanel>
                <WrapPanel x:Name="OTHERS_CONFIG2" Margin="8,0,0,0" Grid.Row="1" VerticalAlignment="Center" Height="38">
                    <WrapPanel x:Name="StopBits_panel2" VerticalAlignment="Center" Width="73" HorizontalAlignment="Center">
                        <Label x:Name="StopBist2" Content="Stop Bits" HorizontalAlignment="Center" VerticalContentAlignment="Center" Padding="7,0,0,0" ScrollViewer.VerticalScrollBarVisibility="Disabled" VerticalAlignment="Center" Foreground="White"/>
                        <ComboBox x:Name="comboBox_StopBits2" Width="67" HorizontalAlignment="Right" SelectionChanged="comboBox_StopBits_SelectionChanged" VerticalContentAlignment="Center" Padding="5,3,0,3" Background="White" VerticalAlignment="Center" Height="23"/>
                    </WrapPanel>
                    <WrapPanel x:Name="DataBits_panel2" VerticalAlignment="Center" Width="73">
                        <Label x:Name="DataBits2" Content="Data Bits" HorizontalAlignment="Center" Padding="7,0,0,0" VerticalContentAlignment="Center" VerticalAlignment="Center" Foreground="White"/>
                        <ComboBox x:Name="comboBox_Databits2" Padding="5,3,0,3" SelectionChanged="comboBox_Databits_SelectionChanged" VerticalContentAlignment="Center" Background="White" Width="67" Height="23"/>
                    </WrapPanel>
                    <WrapPanel x:Name="Parity_panel2" VerticalAlignment="Center" Width="73">
                        <Label x:Name="label2" Content="Parity" HorizontalAlignment="Center" VerticalAlignment="Center" Padding="7,0,0,0" VerticalContentAlignment="Center" Foreground="White"/>
                        <ComboBox x:Name="comboBox_Parity2" HorizontalAlignment="Left" Width="67" Padding="5,0,0,0" VerticalAlignment="Center" SelectionChanged="comboBox_Parity_SelectionChanged" VerticalContentAlignment="Center" Background="White" Height="23"/>
                    </WrapPanel>
                    <WrapPanel x:Name="HandShake_panel2" VerticalAlignment="Center" Width="73">
                        <Label x:Name="handshake2" Content="Flow Control" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Right" VerticalContentAlignment="Center" Padding="0,0,4,0" Foreground="#FFFFF6F6"/>
                        <ComboBox x:Name="comboBox_HandShake2" HorizontalAlignment="Left" Width="67" Padding="5,0,0,0" Height="23" VerticalAlignment="Center" SelectionChanged="comboBox_HandShake_Selected" VerticalContentAlignment="Center" Background="White"/>
                    </WrapPanel>
                </WrapPanel>
                <Button x:Name="Op_Button2" Content="Open" Click="Button_Open1" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0" Background="White" FontWeight="Bold" Grid.Column="1" Padding="15,13" Grid.RowSpan="2" />
            </Grid>
        </Grid>
    </Grid>
</Window>
